 <% if map.bookmarked_by?(current_user) %>
  <%= link_to map.bookmarks.count, map_bookmarks_path(map), method: :delete, local: true, class: "fas fa-star star_yellow" %>
  <%else%>
  <%= link_to map.bookmarks.count, map_bookmarks_path(map), method: :post, local: true, class: "fas fa-star fa-border star_blue" %>
  <%end%>